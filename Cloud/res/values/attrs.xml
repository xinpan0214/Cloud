<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="TitleView">
        <attr name="title" format="string" />
        <attr name="right_visibility" format="boolean" />
    </declare-styleable>
    <declare-styleable name="TabRadioButton">
        <attr name="line_color" format="reference" />
        <attr name="line_height" format="dimension" />
    </declare-styleable>
    <declare-styleable name="PercentBar">
        <attr name="roundColor" format="reference" />
        <attr name="roundWidth" format="dimension" />
        <attr name="max" format="float" />
        <attr name="useSpaceProgress" format="float" />
        <attr name="availableSpaceProgress" format="float" />
        <attr name="percentTextSize" format="dimension" />
        <attr name="useSpaceColor" format="reference" />
        <attr name="usePercentColor" format="reference" />
        <attr name="availableSpaceColor" format="reference" />
        <attr name="availablePercentColor" format="reference" />
    </declare-styleable>


</resources>